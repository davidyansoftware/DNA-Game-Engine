{
  "name": "dna-game-engine",
  "version": "1.0.0",
  "description": "a 2d HTML5 game engine",
  "main": "src/Dna.js",
  "scripts": {
    "build": "parcel build src/Dna.js --out-dir dist",
    "demo": "parcel build demos/shooter/shooter.html --out-dir public -o index.html --public-url ./",
    "shooter": "parcel demos/shooter/shooter.html --out-dir local",
    "platformer": "parcel demos/platformer/platformer.html --out-dir local",
    "astroids": "parcel demos/astroids/astroids.html --out-dir local",
    "collisions": "parcel demos/collisions/collisions.html --out-dir local",
    "hitboxing": "parcel demos/hitboxing/hitboxing.html --out-dir local",
    "camera": "parcel demos/camera/camera.html --out-dir local"
  },
  "author": "David Yan",
  "license": "ISC",
  "devDependencies": {
    "cssnano": "^4.1.10",
    "parcel-bundler": "^1.11.0"
  }
}
